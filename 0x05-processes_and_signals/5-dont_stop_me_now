#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process.

# Find the process ID (PID) of the 4-to_infinity_and_beyond process
process_id=$(pgrep -f "4-to_infinity_and_beyond")

if [ -n "$process_id" ]; then
    # If the process ID is not empty, kill the process
    echo "Stopping 4-to_infinity_and_beyond process with PID $process_id"
    kill "$process_id"
else
    echo "4-to_infinity_and_beyond process is not running"
fi
